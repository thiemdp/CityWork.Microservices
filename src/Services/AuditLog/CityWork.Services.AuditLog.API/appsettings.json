      "ExchangeName": "CityWorkMessage",
      "RoutingKeys": null,
      "QueueNames": null
    },
    "Kafka": {
      "BootstrapServers": "",
      "GroupId": "CityWorkMessage",
      "Topics": null
    }
  },
  "spring": {
    "application": {
      "name": "AuditLogApi"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "false",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "instance": {
      "appName": "AuditLogApi",
      "hostName": "localhost",
      "port": "7146"
    }
